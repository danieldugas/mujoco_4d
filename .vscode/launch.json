{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug MuJoCo simulate",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/simulate", 
        "args": ["${workspaceFolder}/model/humanoid.xml"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb", 
        "miDebuggerPath": "/usr/bin/gdb", 
        "setupCommands": [
          { "description": "Enable pretty-printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
        ]
      }
    ]
  }
  